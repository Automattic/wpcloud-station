{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpcloud/expanding-section",
	"version": "0.1.0",
	"title": "Expanding Section",
	"category": "wpcloud",
	"icon": "list-view",
	"description": "Expanding section",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "wpcloud",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"clickToToggle": {
			"type": "boolean",
			"default": false
		},
		"hideHeader": {
			"type": "boolean",
			"default": true
		},
		"openOnLoad": {
			"type": "boolean",
			"default": false
		},
		"hideContent": {
			"type": "boolean",
			"default": true
		}
	},
	"providesContext": {
		"wpcloud-expanding-section/openOnLoad": "openOnLoad",
		"wpcloud-expanding-section/hideHeader": "hideHeader",
		"wpcloud-expanding-section/hideContent": "hideContent"
	},
	"allowedBlocks": [
		"wpcloud/expanding-section-header",
		"wpcloud/expanding-section-content"
	]
}
