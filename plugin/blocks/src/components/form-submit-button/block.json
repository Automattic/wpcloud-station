{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpcloud/form-submit-button",
	"version": "0.1.0",
	"title": "Form Submit Button",
	"category": "wpcloud",
	"icon": "button",
	"description": "WP Cloud form submit button.",
	"ancestor": [ "wpcloud/form" ],
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "form-submit-button",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"text": {
			"type": "string",
			"default": "Submit"
		},
		"type": {
			"type": "string",
			"default": "submit"
		},
		"asButton": {
			"type": "boolean",
			"default": true
		},
		"outline": {
			"type": "boolean",
			"default": false
		}
	}
}
