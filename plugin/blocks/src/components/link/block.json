{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpcloud/link",
	"version": "0.1.0",
	"title": "Link",
	"category": "wpcloud",
	"icon": "admin-links",
	"description": "The Site Detail block displays a name-value pair for a site detail. The gray italic text is just a placeholder for the actual site detail value.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "site-detail",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"usesContext": [ "wpcloud/layout" ],
	"attributes" : {
		"label": {
			"type": "string",
			"default": "label"
		},
		"name": {
			"type": "string",
			"default": ""
		},
		"adminOnly" :{
			"type": "boolean",
			"default": false
		},
		"url"	: {
			"type": "string",
			"default": ""
		},
		"style": {
			"type": "string"
		},
		"target": {
			"type": "string",
			"default": "_self"
		},
		"download": {
			"type": "boolean",
			"default": false
		},
		"externalLink": {
			"type": "boolean",
			"default": false
		}
	}
}