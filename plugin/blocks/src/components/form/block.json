{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpcloud/form",
	"version": "0.1.0",
	"title": "Form",
	"category": "wpcloud",
	"icon": "feedback",
	"description": "WP Cloud Form",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "wpcloud",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php",
	"attributes": {
		"ajax": {
			"type": "boolean",
			"default": true
		},
		"action": {
			"type": "string"
		},
		"wpcloudAction": {
			"type": "string",
			"default": " "
		},
		"inline": {
			"type": "boolean",
			"default": false
		},
		"redirect": {
			"type": "string"
		},
		"active": {
			"type": "boolean",
			"default": false
		}
	},
	"providesContext": {
		"wpcloud-form/isActive": "active"
	}
}
