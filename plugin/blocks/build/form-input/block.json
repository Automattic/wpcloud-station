{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpcloud/form-input",
  "version": "0.1.0",
  "title": "WP Cloud Form Input",
  "category": "wpcloud",
  "icon": "forms",
  "description": "WP Cloud Form Input block.",
  "example": {},
  "textdomain": "wpcloud",
  "ancestor": [
    "wpcloud/form"
  ],
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "attributes": {
    "type": {
      "type": "string",
      "default": "text"
    },
    "name": {
      "type": "string",
      "default": ""
    },
    "label": {
      "type": "rich-text",
      "default": "Label",
      "selector": ".wpcloud-block-form-input__label-content",
      "source": "rich-text"
    },
    "inlineLabel": {
      "type": "boolean",
      "default": false
    },
    "required": {
      "type": "boolean",
      "default": false,
      "selector": ".wpcloud-block-form-input__input",
      "source": "attribute",
      "attribute": "required"
    },
    "adminOnly": {
      "type": "boolean",
      "default": false
    },
    "placeholder": {
      "type": "string",
      "selector": ".wpcloud-block-form-input__input",
      "source": "attribute",
      "attribute": "placeholder"
    },
    "options": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "value": {
      "type": "string",
      "default": "",
      "selector": "input",
      "source": "attribute",
      "attribute": "value"
    }
  },
  "supports": {
    "anchor": true,
    "reusable": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    },
    "__experimentalBorder": {
      "radius": true,
      "__experimentalSkipSerialization": true,
      "__experimentalDefaultControls": {
        "radius": true
      }
    }
  }
}