{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpcloud/site-template",
  "title": "Site Template",
  "category": "wpcloud",
  "parent": [
    "core/query"
  ],
  "description": "Contains the block elements used to render a site.",
  "textdomain": "wpcloud",
  "usesContext": [
    "queryId",
    "query",
    "displayLayout",
    "templateSlug",
    "previewPostType",
    "enhancedPagination"
  ],
  "supports": {
    "reusable": false,
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "layout": true,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "render": "file:./render.php",
  "editorScript": "file:./index.js"
}