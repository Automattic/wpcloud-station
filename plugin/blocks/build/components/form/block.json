{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpcloud/form",
  "version": "0.1.0",
  "title": "WP Cloud Form",
  "category": "wpcloud",
  "icon": "feedback",
  "description": "WP Cloud Form",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "wpcloud",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php",
  "attributes": {
    "ajax": {
      "type": "boolean",
      "default": false
    },
    "action": {
      "type": "string"
    },
    "wpcloudAction": {
      "type": "string"
    },
    "inline": {
      "type": "boolean",
      "default": false
    },
    "redirect": {
      "type": "string"
    }
  },
  "allowedBlocks": [
    "wpcloud/site-detail",
    "core/paragraph",
    "core/heading",
    "core/group",
    "core/image",
    "core/columns",
    "core/grid",
    "wpcloud/form-input",
    "wpcloud/button"
  ]
}